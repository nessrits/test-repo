{
  "name": "webdriverIO-with-cucumberBDD",
  "description": "WebDriver bindings for Node.js with Cucumber",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/webdriverio/bin/wdio ./suite.cucumber.conf.js",
